//---------Part B-------------------
import java.io.*;
class Student implements Serializable {
  int id; String name; String grade;
  Student(int i, String n, String g) { id=i; name=n; grade=g; }
}
class StudentTest {
  public static void main(String[] a) throws Exception {
    Student s1 = new Student(101,"Alice","A");
    ObjectOutputStream o = new ObjectOutputStream(new FileOutputStream("student.dat"));
    o.writeObject(s1); o.close();
    ObjectInputStream in = new ObjectInputStream(new FileInputStream("student.dat"));
    Student s = (Student) in.readObject(); in.close();
    System.out.println(s.id+" "+s.name+" "+s.grade);
  }
}
